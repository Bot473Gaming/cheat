// Lưu ý code này chỉ hoạt động khi đã làm xong bài :3
// By nameless
var ans_data= JSON.parse($("#data-ans").html());
var s = null;
var ans = ['A','B','C','D','E','F'];
for(i=0;i<ans_data.length;i++)
    s = s+' '+ans_data[i];
s = s.split(' ');
res = 'DAP AN\n';
for(i = 1;i<s.length;i++)
    res += `Cau ${i} : ${ans[s[i]-1]}\n`;
console.log(res);
